@using StackExchange.Profiling;
@this.InitClientTimings()

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    @* optional time scripts in the header *@ 
@*
	  @this.TimeScript("Content Site.css", 
        @<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />)
    @this.TimeScript("jQuery 1.5.1", 
        @<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>)
    @this.TimeScript("modernizr", 
        @<script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>)
*@

    <link href="@Links.styles.reset_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.style_css" rel="stylesheet" type="text/css" />

    <link href="@Links.styles.jquery_ui_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_accordion_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_all_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_autocomplete_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_base_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_button_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_core_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_datepicker_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_dialog_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_progressbar_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_resizable_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_selectable_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_slider_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_tabs_css" rel="stylesheet" type="text/css" />
    <link href="@Links.styles.jquery_ui_theme_css" rel="stylesheet" type="text/css" />

    <link href="http://harvesthq.github.com/chosen/chosen/chosen.css" rel="stylesheet" type="text/css" />
    <script src="@Links.scripts.modernizr_1_7_js" type="text/javascript"></script>
    <script src="@Links.scripts.jquery_1_7_2_js" type="text/javascript"></script>
    <script src="@Links.scripts.jquery_ui_1_8_16_js" type="text/javascript"></script>
    <script src="@Links.scripts.jquery_ui_1_8_16_datepicker_min_js" type="text/javascript"></script>
    <script src="@Links.scripts.jquery_unobtrusive_ajax_js" type="text/javascript"></script>
    <script src="http://ajax.microsoft.com/ajax/jquery.templates/beta1/jquery.tmpl.min.js" type="text/javascript"></script>
    <script src="@Links.scripts.jquery_js" type="text/javascript"></script>
    <script src="http://harvesthq.github.com/chosen/chosen/chosen.jquery.js" type="text/javascript"></script>

    @StackExchange.Profiling.MiniProfiler.RenderIncludes(StackExchange.Profiling.RenderPosition.Right)

	<style type="text/css">
	#upload-form 
	{
		width:0px;
	}
	#upload-form input 
	{
		opacity:0;
		width:0px;
	}
	</style>
</head>
<body>
  <div class="page">
    <nav><ul>
      <li>@Html.ActionLink("Accounts", MVC.Account.Accounts())</li>
      <li>@Html.ActionLink("Transactions", MVC.Account.Transactions())</li>
      <li>@Html.ActionLink("Vendors", MVC.Account.Vendors())</li>
      <li>@Html.ActionLink("Categories", MVC.Account.Categories())</li>
      <li>@Html.ActionLink("Bills", MVC.Account.Bills())</li>
      <li>@Html.ActionLink("Budget", MVC.Account.Budget())</li>
	  <li><a href="javascript:void();">@using (Html.BeginForm(MVC.Account.UploadTransactions(), FormMethod.Post, new { id="upload-form", enctype = "multipart/form-data", style ="display:inline-block;" })){<input type="file" id="file" name="file" onchange="$('#upload-form').submit();" />}Upload</a></li>
    </ul></nav>
    <section id="main">
      @RenderBody()
    </section>
    <footer>
    </footer>
  </div>
</body>
</html>
